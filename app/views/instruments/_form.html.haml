= simple_form_for([@session, @session.instruments.build]) do |f|
  = f.input :name, input_html: {class: "form-control"}
  = f.input :exercise, input_html: {class: "form-control"}
  = f.input :tempo, input_html: {class: "form-control"}
  = f.input :notes, input_html: {class: "form-control"}
  %br/
  = f.button :submit
